    "compilerOptions": {
        "noImplicitAny": true,
        "target": "es5",
        "skipLibCheck": true,
        "lib": ["es6", "DOM", "ScriptHost"],
        "downlevelIteration": true,
        "resolveJsonModule": true
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.js"
    ]
}
